    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.17.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.9.2",
    "vite": "^6.0.2",
    "vitest": "^3.2.4"

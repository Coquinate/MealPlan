{
  "name": "@coquinate/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./src/utils/constants": {
      "types": "./dist/utils/constants.d.ts",
      "import": "./dist/utils/constants.js"
    }
  },
  "scripts": {
    "build": "tsc && echo 'Shared build complete'",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "echo 'To be configured'",

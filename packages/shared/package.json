  "devDependencies": {
    "@coquinate/config": "workspace:*",
    "@types/lz-string": "^1.5.0",
    "@types/node": "^20.11.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.0"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.18",
    "@supabase/supabase-js": "^2.39.3",
    "ai": "^4.2.2",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "lz-string": "^1.5.0",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.25.76",
    "zustand": "^5.0.2"
  }
